{
	"name": "redfoxy/urlopenstonewwindow",
	"type": "phpbb-extension",
	"description": "An extension that change post's links [url][/url] target to a new window.",
	"homepage": "http://www.redfoxy.it",
	"version": "1.0.0",
	"time": "2015-03-05",
	"keywords": ["phpbb", "extension", "url", "new", "window", "target"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Massimo 'RedFoxy Darrest' Ciccio'",
			"homepage": "http://www.redfoxy.it",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"extra": {
		"display-name": "URL tags opens in a new window",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0"
		},
		"version-check": {
			"host": "www.phpbb.com",
			"directory": "/customise/db/extension/urlopenstonewwindow",
			"filename": "version_check"
		}
	}
}
